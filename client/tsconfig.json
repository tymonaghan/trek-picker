//write a tsconfig file with module interop enabled
{
  "include": [
    "src/**/*",
    "test/**/*",
    "App.tsx",
    "/client",
    "client"
  ],
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES5",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "jsx": "react",
    "sourceMap": true
  }
}
